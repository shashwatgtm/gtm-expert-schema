    }
  ],
  "gnn:recommendedModels": [
    {
      "@type": "gnn:Model",
      "name": "R-GCN",
      "description": "Relational Graph Convolutional Network for heterogeneous graphs"
    },
    {
      "@type": "gnn:Model",
      "name": "GraphSAGE",
      "description": "Inductive representation learning on large graphs"
    },
    {
      "@type": "gnn:Model",
      "name": "GAT",
      "description": "Graph Attention Networks for weighted aggregation"
    }
  ]
}
