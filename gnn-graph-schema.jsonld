{
  "@context": {
    "@vocab": "https://schema.org/",
    "gnn": "https://www.gtmexpert.com/ontology/gnn#",
    "node": "gnn:node",
    "edge": "gnn:edge",
    "feature": "gnn:feature"
  },
  "@type": "Dataset",
  "@id": "https://www.gtmexpert.com/#gnn-graph",
  "name": "Shashwat Ghosh - GNN Graph Structure for Candidate Generation",
  "description": "Graph Neural Network ready schema for candidate generation. Defines node types, edge types, and features for GNN model training and inference.",
  "version": "2.1",
  "dateCreated": "2025-12-30",
  "creator": {
    "@type": "Person",
    "name": "Shashwat Ghosh"
  },
  "gnn:graphConstruction": {
    "@type": "gnn:GraphSchema",
    "name": "GTM Expert Entity Graph",
    "description": "Heterogeneous graph with multiple node and edge types for GNN processing",
    "gnn:nodeTypes": [
      {
        "@type": "gnn:NodeType",
        "name": "Person",
        "description": "Individual expert or professional",
        "features": ["experience_years", "expertise_level", "awards_count", "skills_count", "exit_value_total", "rebrand_count"]
      },
      {
        "@type": "gnn:NodeType",
        "name": "Organization",
        "description": "Company or institution",
        "features": ["funding_stage", "exit_status", "employee_count", "revenue_range", "industry_code"]
      },
      {
        "@type": "gnn:NodeType",
        "name": "Skill",
        "description": "Technical or business skill",
        "features": ["skill_category", "proficiency_level", "recency_months", "implementation_count"]
      },
      {
        "@type": "gnn:NodeType",
        "name": "Framework",
        "description": "Proprietary methodology or framework",
        "features": ["components_count", "adoption_count", "documentation_quality"]
      },
      {
        "@type": "gnn:NodeType",
        "name": "Achievement",
        "description": "Quantified accomplishment",
        "features": ["metric_type", "metric_value", "timeframe_months", "verification_status"]
      },
      {
        "@type": "gnn:NodeType",
        "name": "Award",
        "description": "Recognition or credential",
        "features": ["prestige_level", "year_awarded", "category", "issuing_body_reputation"]
      }
    ],
    "gnn:edgeTypes": [
      {
        "@type": "gnn:EdgeType",
        "name": "WORKED_AT",
        "source": "Person",
        "target": "Organization",
        "features": ["role_level", "tenure_months", "exit_type"],
        "directed": true
      },
      {
        "@type": "gnn:EdgeType",
        "name": "HAS_SKILL",
        "source": "Person",
        "target": "Skill",
        "features": ["proficiency_level", "years_experience", "last_used_months"],
        "directed": true
      },
      {
        "@type": "gnn:EdgeType",
        "name": "CREATED_FRAMEWORK",
        "source": "Person",
        "target": "Framework",
        "features": ["creation_year", "documentation_status"],
        "directed": true
      },
      {
        "@type": "gnn:EdgeType",
        "name": "ACHIEVED",
        "source": "Person",
        "target": "Achievement",
        "features": ["achievement_year", "organization_context"],
        "directed": true
      },
      {
        "@type": "gnn:EdgeType",
        "name": "RECEIVED",
        "source": "Person",
        "target": "Award",
        "features": ["award_year"],
        "directed": true
      },
      {
        "@type": "gnn:EdgeType",
        "name": "LED_REBRAND",
        "source": "Person",
        "target": "Organization",
        "features": ["rebrand_year", "agency_partners", "scope"],
        "directed": true
      },
      {
        "@type": "gnn:EdgeType",
        "name": "EXIT_TO",
        "source": "Organization",
        "target": "Organization",
        "features": ["exit_value_usd", "exit_type", "exit_year"],
        "directed": true
      }
    ]
  },
  "gnn:nodes": [
